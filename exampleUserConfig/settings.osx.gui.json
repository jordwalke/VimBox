{
  "*": {
    "vim": {
      "mappings": {
        "insert:<D-f>": "keys:<Esc>/",
        "normal:<D-f>": "keys:<Esc>/",
        "all:<D-w>": "command:confirm close",
        "all:<D-S-t>": "command:UndoQuit"
      }
    },
    "Gundo": {
      "mappings": {
        "all:<D-U>": "command:GundoToggle"
      }
    }
  },
  "reason": {
    "vimreason": {
      "mappings": {
        "all:<D-C>": "command:ReasonPrettyPrint"
      }
    }
  }
}

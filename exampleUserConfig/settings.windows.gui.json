{
  "*": {
    "installer": {
      "config": {
        "misc": "xolox/vim-misc",
        "shell": "xolox/vim-shell",
        "bargreybars": "dreadnaut/vim-bargreybars",
        "winPython": "file:$vimBoxStockPluginsDir/vim-win-python",
        "winbox": "file:$vimBoxStockPluginsDir/vim-winbox"
      }
    },
    "winbox": {
      "config": {
      },
      "mappings": {
        "all:<F11>": "command:call WinBoxToggleFullScreen()",
        "all:<c-up>": "command:call WinBoxIncreaseFont()",
        "all:<c-down>": "command:call WinBoxDecreaseFont()"
      }
    },
    "vim": {
      "Notes": "pythonthreehome option is only made available in new/bleeding edge gvim. For now set the PYTHONHOME variable.",
      "Notes": "http://unix.stackexchange.com/questions/44616/why-is-vim-creating-files-with-dos-line-endings",
      "config": {
        "fileformats": "unix,dos",
        "guioptions": "gmMk",
        "$PYTHONHOME": "eval:has(\"win64\") ? PathJoinDir(g:vimBoxStockPluginsDir, 'python-3.7.0-embed-amd64') : PathJoinDir(g:vimBoxStockPluginsDir, 'python-3.7.0-embed-win32')",
        "pythonthreedll": "eval:PathJoinFile($PYTHONHOME, 'python37.dll')"
      },
      "mappings": {
        "all:<F11>": "command:call WinBoxToggleFullScreen()",
        "all:<c-s>": "command:w",
        "all:<c-n>": "command:enew",
        "all:Ü": "command:NERDTreeToggle",
        "all:ü": "command:NERDTreeFind"
      }
      }
    },
    "nerdcommenter": {
      "mappings": {
        "visualRemapped selectRemapped:<F15>": "keys:<Plug>NERDCommenterToggle",
        "normalRemapped:<F15>": "keys:<Plug>NERDCommenterToggle"
      }
    },
    "shell": {
      "config": {
        "fullscreen_always_on_top": false,
        "fullscreen_items": "mT",
        "fullscreen_message": "",
        "shell_mappings_enabled": 0
      }
    },
    "vimBox": {
      "config": {
        "useDevIcons": false
      }
    }
  },
  "reason": {
    "vim": {
      "mappings": {
        "all:Ã": "command:ReasonPrettyPrint"
      }
    }
  }
}
